{
  "name": "yz-app",
  "version": "1.0.0-rc.0",
  "description": "YunzaiJS APP",
  "author": "yunzaijs",
  "type": "module",
  "private": true,
  "scripts": {
    "app": "node lib/main.js",
    "dev": "npx lvy dev",
    "view": "npx lvy dev --view",
    "build": "npx lvy build",
    "start": "npx pm2 startOrRestart pm2.config.cjs",
    "stop": "npx pm2 stop pm2.config.cjs",
    "delete": "npx pm2 delete pm2.config.cjs"
  },
  "dependencies": {
    "jsxp": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^22",
    "icqq": "^0.6.10",
    "lvyjs": "^0.2.25",
    "prettier": "^3.0.3",
    "yunzaijs": "^1.0.0-rc.5",
    "cssnano": "^7.0.6"
  },
  "types": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "keywords": [
    "yunzaijs"
  ],
  "workspaces": [
    "packages/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
