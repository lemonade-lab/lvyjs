{
  "name": "lvyjs",
  "version": "0.2.11",
  "description": "tsx compile script",
  "author": "lemonade",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-styles": "^4.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.3"
  },
  "types": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./loader": {
      "import": "./lib/loader.js",
      "types": "./lib/loader.d.ts"
    },
    "./register": {
      "import": "./lib/main.js",
      "types": "./lib/main.d.ts"
    },
    "./env": {
      "types": "./env.d.ts"
    },
    "./tsconfig.json": {
      "import": "./tsconfig.json",
      "types": "./tsconfig.json"
    }
  },
  "bin": {
    "lvy": "bin/index.js"
  },
  "keywords": [
    "ts",
    "tsx",
    "react"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "bugs": "https://github.com/lvyjs/core/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvyjs/core.git"
  }
}
