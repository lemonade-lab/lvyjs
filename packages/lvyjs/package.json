{
  "name": "lvyjs",
  "version": "0.2.0",
  "description": "tsx compile script",
  "author": "lemonade",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "esbuild": "^0.24.0",
    "logger": "^0.0.1",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.2.0",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.9",
    "postcss-simple-vars": "^7.0.1",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "postcss-url": "^10.1.3",
    "json5": "^2.2.3",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-styles": "^4.0.0",
    "sass": "^1.79.4",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.19.1",
    "typescript": "^5.5.3"
  },
  "types": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./loader": {
      "import": "./lib/loader.js",
      "types": "./lib/loader.d.ts"
    },
    "./register": {
      "import": "./lib/main.js",
      "types": "./lib/main.d.ts"
    },
    "./env": {
      "types": "./env.d.ts"
    },
    "./tsconfig.json": {
      "import": "./tsconfig.json",
      "types": "./tsconfig.json"
    }
  },
  "bin": {
    "lvy": "bin/index.js"
  },
  "keywords": [
    "ts",
    "tsx",
    "react"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "bugs": "https://github.com/lemonade-lab/lvy/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonade-lab/lvy.git"
  }
}
